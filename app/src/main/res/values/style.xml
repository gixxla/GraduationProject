<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="title1">
        <item name="android:textSize">32sp</item>
        <item name="android:textColor">#000000</item>
        <item name="android:fontFamily">@font/intersemibold</item>
    </style>
    <style name="title2">
        <item name="android:textSize">20sp</item>
